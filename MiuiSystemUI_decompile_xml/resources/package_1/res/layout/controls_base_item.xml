<?xml version='1.0' encoding='utf-8' ?>
<androidx.constraintlayout.widget.ConstraintLayout android:background="@drawable/control_background"
                      android:padding="@dimen/control_padding"
                      android:focusable="true"
                      android:clickable="false"
                      android:layout_width="0.0dp"
                      android:layout_height="@dimen/control_height"
                      android:layout_weight="1.0"
                      android:layout_marginStart="@dimen/control_spacing"
                      android:stateListAnimator="@anim/control_state_list_animator"
                      android:screenReaderFocusable="true" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
  <ImageView android:id="@id/icon"
             android:paddingTop="@dimen/control_padding_adjustment"
             android:focusable="false"
             android:clickable="false"
             android:layout_width="@dimen/control_icon_size"
             android:layout_height="@dimen/control_icon_size"
             app:r0x7f040443="0"
             app:r0x7f040447="0" />
  <TextView android:textAppearance="@style/TextAppearance.Control.Status"
            android:ellipsize="marquee"
            android:id="@id/status"
            android:paddingTop="@dimen/control_padding_adjustment"
            android:focusable="false"
            android:clickable="false"
            android:layout_width="0.0dp"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:marqueeRepeatLimit="marquee_forever"
            android:textDirection="locale"
            android:paddingStart="@dimen/control_status_padding"
            android:screenReaderFocusable="false"
            app:r0x7f040429="@id/icon"
            app:r0x7f04042f="0"
            app:r0x7f040442="@id/icon" />
  <TextView android:textAppearance="@style/TextAppearance.Control.Title"
            android:ellipsize="end"
            android:id="@id/id_0x7f0a09a2"
            android:paddingLeft="@dimen/control_padding_adjustment"
            android:paddingRight="@dimen/control_padding_adjustment"
            android:focusable="false"
            android:clickable="false"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:textDirection="locale"
            app:r0x7f04042a="@id/barrier"
            app:r0x7f04042f="0"
            app:r0x7f040443="0" />
  <androidx.constraintlayout.widget.Barrier android:id="@id/barrier"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:r0x7f0400c7="2"
                        app:r0x7f0401da="subtitle,favorite" />
  <TextView android:textAppearance="@style/TextAppearance.Control.Subtitle"
            android:ellipsize="end"
            android:id="@id/subtitle"
            android:paddingLeft="@dimen/control_padding_adjustment"
            android:paddingRight="@dimen/control_padding_adjustment"
            android:paddingBottom="@dimen/control_padding_adjustment"
            android:focusable="false"
            android:clickable="false"
            android:layout_width="0.0dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:textDirection="locale"
            app:r0x7f040430="@id/favorite"
            app:r0x7f040443="0"
            app:r0x7f040447="@id/favorite" />
  <CheckBox android:gravity="center"
            android:id="@id/favorite"
            android:background="@android:color/transparent"
            android:visibility="invisible"
            android:clickable="false"
            android:layout_width="@dimen/controls_management_checkbox_size"
            android:layout_height="@dimen/controls_management_checkbox_size"
            android:minWidth="0.0dp"
            android:minHeight="0.0dp"
            android:selectable="false"
            android:importantForAccessibility="no"
            app:r0x7f040429="0"
            app:r0x7f04042f="0"
            app:r0x7f040442="@id/subtitle" />
  <ImageView android:id="@id/chevron_icon"
             android:focusable="false"
             android:visibility="invisible"
             android:clickable="false"
             android:layout_width="@dimen/control_chevron_icon_size"
             android:layout_height="@dimen/control_chevron_icon_size"
             android:src="@drawable/ic_chevron_icon"
             android:autoMirrored="true"
             app:r0x7f040429="0"
             app:r0x7f04042f="0"
             app:r0x7f040447="0" />
</androidx.constraintlayout.widget.ConstraintLayout>