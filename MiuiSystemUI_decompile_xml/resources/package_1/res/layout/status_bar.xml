<?xml version='1.0' encoding='utf-8' ?>
<com.android.systemui.statusbar.phone.MiuiPhoneStatusBarView android:orientation="vertical"
                      android:id="@id/status_bar"
                      android:focusable="false"
                      android:clipChildren="false"
                      android:clipToPadding="false"
                      android:descendantFocusability="afterDescendants"
                      android:layout_width="fill_parent"
                      android:layout_height="@dimen/status_bar_height"
                      android:accessibilityPaneTitle="@string/status_bar" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:systemui="http://schemas.android.com/apk/res-auto">
  <com.android.systemui.statusbar.views.BatteryIndicator android:id="@id/battery_indicator"
                        android:visibility="gone"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/battery_line_height"
                        android:src="@drawable/battery_indicator" />
  <com.android.systemui.statusbar.phone.MiuiHomePrivacyView android:layout_gravity="center|end"
                        android:orientation="horizontal"
                        android:id="@id/home_privacy_container"
                        android:paddingTop="@dimen/status_bar_padding_top"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingEnd="@dimen/status_bar_padding_end" />
  <LinearLayout android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@id/status_bar_contents"
                android:paddingTop="@dimen/status_bar_padding_top"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:paddingStart="@dimen/status_bar_padding_start"
                android:paddingEnd="@dimen/status_bar_padding_end">
    <LinearLayout android:id="@id/phone_status_bar_left_container"
                  android:clipChildren="false"
                  android:layout_width="wrap_content"
                  android:layout_height="fill_parent">
      <include layout="@layout/heads_up_status_bar_layout" />
      <com.android.systemui.statusbar.views.LimitedSizeFrameLayout android:layout_gravity="center_vertical"
                            android:id="@id/prompt_container"
                            android:visibility="gone"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/status_bar_icon_height"
                            systemui:r0x7f040003="@dimen/status_bar_prompt_max_width" />
      <ViewStub android:id="@id/operator_name"
                android:layout="@layout/operator_name"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent" />
      <com.android.systemui.statusbar.views.MiuiStatusBarClock android:textAppearance="@style/TextAppearance.StatusBar.Clock"
                            android:gravity="center|start"
                            android:id="@id/clock"
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:singleLine="true"
                            android:layout_marginEnd="4.0dp"
                            systemui:r0x7f040000="0"
                            systemui:r0x7f040001="0" />
      <ViewStub android:id="@id/pad_clock"
                android:layout="@layout/pad_clock"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_marginEnd="4.0dp" />
      <com.android.systemui.statusbar.lyrics.MiuiStatusLyricView android:textAppearance="@style/TextAppearance.StatusBar.Clock"
                            android:ellipsize="marquee"
                            android:gravity="center|start"
                            android:focusable="true"
                            android:visibility="gone"
                            android:scrollHorizontally="true"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:maxWidth="220dp"
                            android:singleLine="true"
                            android:marqueeRepeatLimit="1"
                            android:layout_marginEnd="4dp" />
      <com.android.systemui.statusbar.AlphaOptimizedFrameLayout android:orientation="horizontal"
                            android:id="@id/drip_notification_icon_area"
                            android:visibility="gone"
                            android:clipChildren="false"
                            android:layout_width="0.0dp"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0" />
    </LinearLayout>
    <android.widget.Space android:gravity="center"
                          android:id="@id/cutout_space_view"
                          android:layout_width="0.0dp"
                          android:layout_height="fill_parent" />
    <com.android.systemui.statusbar.views.MiuiNotificationStatusContainer android:id="@id/system_icon_area"
                          android:layout_width="fill_parent"
                          android:layout_height="fill_parent">
      <com.android.systemui.statusbar.AlphaOptimizedFrameLayout android:id="@id/fullscreen_notification_icon_area"
                            android:clipChildren="false"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent" />
      <com.android.systemui.statusbar.phone.userswitcher.StatusBarUserSwitcherContainer android:orientation="horizontal"
                            android:id="@id/user_switcher_container"
                            android:visibility="gone"
                            android:layout_width="0.0dp"
                            android:layout_height="0.0dp">
        <ImageView android:id="@id/current_user_avatar"
                   android:layout_width="0.0dp"
                   android:layout_height="0.0dp"
                   android:scaleType="centerInside" />
        <TextView android:textAppearance="@style/TextAppearance.StatusBar.Clock"
                  android:id="@id/current_user_name"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content" />
      </com.android.systemui.statusbar.phone.userswitcher.StatusBarUserSwitcherContainer>
      <include android:id="@id/system_icons"
               android:layout_width="wrap_content"
               android:layout_height="fill_parent"
               layout="@layout/system_icons" />
      <LinearLayout android:orientation="horizontal"
                    android:id="@id/privacy_container"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginStart="2.0dp" />
    </com.android.systemui.statusbar.views.MiuiNotificationStatusContainer>
    <FrameLayout android:id="@id/status_bar_start_side_content"
                 android:visibility="gone"
                 android:clipChildren="false"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />
    <FrameLayout android:id="@id/status_bar_end_side_content"
                 android:visibility="gone"
                 android:clipChildren="false"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content" />
  </LinearLayout>
  <com.android.systemui.statusbar.phone.FocusedNotifPromptView android:gravity="center_vertical"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@id/focused_notif_view"
                        android:paddingTop="@dimen/status_bar_padding_top"
                        android:visibility="gone"
                        android:clipChildren="false"
                        android:clipToPadding="false"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:importantForAccessibility="no"
                        android:paddingStart="@dimen/focused_notif_view_padding_start">
    <com.android.systemui.statusbar.phone.FocusedNotifPromptImageView android:id="@id/focused_notif_icon"
                          android:layout_width="@dimen/focused_notif_icon_size"
                          android:layout_height="@dimen/focused_notif_icon_size"
                          android:scaleType="centerInside" />
    <FrameLayout android:id="@id/focused_notif_content"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_marginStart="5.0dp">
      <TextView android:textAppearance="@style/TextAppearance.StatusBar.Clock"
                android:ellipsize="end"
                android:id="@id/focused_notif_content_text"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxEms="6"
                android:singleLine="true"
                android:fontFamily="MiSans-normal" />
      <com.android.systemui.statusbar.views.LimitedSizeFrameLayout android:id="@id/focused_notif_content_remote"
                            android:visibility="gone"
                            android:clipChildren="false"
                            android:clipToPadding="false"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            systemui:r0x7f040003="@dimen/status_bar_prompt_max_width" />
    </FrameLayout>
  </com.android.systemui.statusbar.phone.FocusedNotifPromptView>
</com.android.systemui.statusbar.phone.MiuiPhoneStatusBarView>