<?xml version='1.0' encoding='utf-8' ?>
<set xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
  <objectAnimator android:interpolator="@android:anim/accelerate_decelerate_interpolator"
                  android:duration="300"
                  android:startOffset="15"
                  android:valueFrom="3"
                  android:valueTo="-3"
                  android:valueType="floatType"
                  android:propertyName="translateY" />
  <objectAnimator android:interpolator="@android:interpolator/fast_out_slow_in"
                  android:duration="200"
                  android:startOffset="315"
                  android:valueFrom="-3"
                  android:valueTo="0"
                  android:valueType="floatType"
                  android:propertyName="translateY" />
</set>