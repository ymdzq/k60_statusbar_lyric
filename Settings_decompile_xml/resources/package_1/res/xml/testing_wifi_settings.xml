<?xml version='1.0' encoding='utf-8' ?>
<PreferenceScreen android:title="@string/testing_wifi_info"
                  android:key="parent" xmlns:android="http://schemas.android.com/apk/res/android">
  <com.android.settingslib.miuisettings.preference.Preference android:title="@string/wifi_api_test">
    <intent android:targetPackage="com.android.settings"
            android:action="android.intent.action.MAIN"
            android:targetClass="com.android.settings.Settings$WifiAPITestActivity" />
  </com.android.settingslib.miuisettings.preference.Preference>
  <com.android.settingslib.miuisettings.preference.Preference android:title="@string/wifi_config_info">
    <intent android:targetPackage="com.android.settings"
            android:action="android.intent.action.MAIN"
            android:targetClass="com.android.settings.wifi.WifiConfigInfo" />
  </com.android.settingslib.miuisettings.preference.Preference>
  <com.android.settingslib.miuisettings.preference.Preference android:title="@string/wifi_status_test">
    <intent android:targetPackage="com.android.settings"
            android:action="android.intent.action.MAIN"
            android:targetClass="com.android.settings.wifi.WifiStatusTest" />
  </com.android.settingslib.miuisettings.preference.Preference>
</PreferenceScreen>