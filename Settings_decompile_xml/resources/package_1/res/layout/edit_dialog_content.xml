<?xml version='1.0' encoding='utf-8' ?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
  <FrameLayout android:gravity="center"
               android:orientation="vertical"
               android:id="@id/topPanel"
               android:layout_width="fill_parent"
               android:layout_height="wrap_content"
               android:minHeight="@dimen/miuix_appcompat_alert_dialog_title_min_height">
    <LinearLayout android:gravity="center"
                  android:background="?attr/actionBarBackground"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content">
      <Button android:id="@id/cancel"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="@android:string/cancel"
              style="?attr/actionModeCancelButtonStyle" />
      <TextView android:ellipsize="end"
                android:gravity="center"
                android:id="@id/alertTitle"
                android:layout_width="0.0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                style="?android:attr/windowTitleStyle" />
      <Button android:id="@id/select"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              style="?attr/actionModeSelectButtonStyle" />
    </LinearLayout>
  </FrameLayout>
  <LinearLayout android:orientation="vertical"
                android:id="@id/contentPanel"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">
    <ScrollView android:id="@id/id_0x7f0a0936"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:overScrollMode="ifContentScrolls">
      <TextView android:id="@id/message"
                android:paddingTop="8.0dp"
                android:paddingBottom="8.0dp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingStart="16.0dp"
                android:paddingEnd="16.0dp"
                style="?android:attr/textAppearanceMedium" />
    </ScrollView>
  </LinearLayout>
  <FrameLayout android:id="@id/customPanel"
               android:layout_width="fill_parent"
               android:layout_height="wrap_content"
               android:layout_weight="1.0">
    <FrameLayout android:id="@android:id/custom"
                 android:layout_width="fill_parent"
                 android:layout_height="wrap_content" />
  </FrameLayout>
  <FrameLayout android:id="@id/checkboxPanel"
               android:paddingTop="@dimen/miuix_appcompat_dialog_checkbox_vertical_padding"
               android:layout_width="fill_parent"
               android:layout_height="wrap_content"
               android:paddingStart="@dimen/miuix_appcompat_dialog_checkbox_horizontal_padding"
               android:paddingEnd="@dimen/miuix_appcompat_dialog_checkbox_horizontal_padding">
    <CheckBox android:id="@android:id/checkbox"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              style="?attr/dialogCheckBoxStyle" />
  </FrameLayout>
  <LinearLayout android:orientation="vertical"
                android:id="@id/buttonPanel"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:minHeight="50.0dp">
    <LinearLayout android:orientation="horizontal"
                  android:id="@id/buttonGroup"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:baselineAligned="false"
                  android:measureWithLargestChild="true"
                  style="?android:attr/buttonBarStyle">
      <Button android:gravity="center"
              android:id="@android:id/button2"
              android:layout_width="0.0dp"
              android:layout_height="wrap_content"
              android:maxLines="2"
              android:layout_weight="1.0"
              style="?android:attr/buttonBarButtonStyle" />
      <Button android:gravity="center"
              android:id="@android:id/button3"
              android:layout_width="0.0dp"
              android:layout_height="wrap_content"
              android:maxLines="2"
              android:layout_weight="1.0"
              style="?android:attr/buttonBarButtonStyle" />
      <Button android:gravity="center"
              android:id="@android:id/button1"
              android:layout_width="0.0dp"
              android:layout_height="wrap_content"
              android:maxLines="2"
              android:layout_weight="1.0"
              style="?attr/buttonBarDefaultButtonStyle" />
    </LinearLayout>
  </LinearLayout>
</merge>