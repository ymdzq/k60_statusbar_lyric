<?xml version='1.0' encoding='utf-8' ?>
<FrameLayout android:gravity="center"
             android:id="@id/check_password_container"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
  <LinearLayout android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="56.0dp"
                android:layout_marginTop="25.0dp"
                android:paddingStart="5.67dp"
                android:paddingEnd="5.67dp">
    <TextView android:id="@id/cancel_finger_authenticate"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:drawableStart="@drawable/miuix_appcompat_action_bar_back_dark" />
  </LinearLayout>
  <LinearLayout android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="127.3dp"
                android:layout_marginStart="20.0dp"
                android:layout_marginEnd="20.0dp">
    <TextView android:textSize="@dimen/fod_authentication_dialog_title"
              android:textStyle="bold"
              android:textColor="@color/fod_authentication_dialog_title"
              android:gravity="center"
              android:layout_gravity="center_horizontal"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="@string/fingerprint_identify_msg" />
    <TextView android:textSize="@dimen/fod_authentication_dialog_subtitle"
              android:textColor="@color/fod_authentication_dialog_title"
              android:gravity="center"
              android:layout_gravity="center_horizontal"
              android:id="@id/confirm_fingerprint_view_msg"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginTop="9.5dp"
              android:text="@string/confirm_fingerprint_msg" />
  </LinearLayout>
  <ImageView android:layout_gravity="center_horizontal"
             android:id="@id/back_finger_print"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginTop="282.0dp"
             android:src="@drawable/back_finger_print" />
</FrameLayout>