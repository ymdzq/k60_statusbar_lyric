<?xml version='1.0' encoding='utf-8' ?>
<LinearLayout android:gravity="center_vertical"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:minHeight="@dimen/miuix_appcompat_list_preferred_item_height_large"
              android:paddingEnd="?android:attr/scrollbarSize" xmlns:android="http://schemas.android.com/apk/res/android">
  <LinearLayout android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@id/title_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14.2dp"
                android:layout_marginBottom="8.0dp"
                android:paddingStart="29.2dp"
                android:paddingEnd="29.2dp">
    <ImageView android:layout_gravity="center"
               android:id="@android:id/icon"
               android:layout_width="25.1dp"
               android:layout_height="21.8dp" />
    <TextView android:textSize="@dimen/key_words_text_size"
              android:ellipsize="marquee"
              android:id="@android:id/title"
              android:fadingEdge="horizontal"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="mipro-medium"
              android:layout_marginStart="11.6dp" />
    <TextView android:textSize="@dimen/key_words_text_size"
              android:ellipsize="marquee"
              android:id="@android:id/summary"
              android:fadingEdge="horizontal"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="mipro-medium"
              android:layout_marginStart="11.6dp" />
  </LinearLayout>
  <miuix.androidbasewidget.widget.SeekBar android:id="@id/seekbar"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" />
</LinearLayout>