<?xml version='1.0' encoding='utf-8' ?>
<RelativeLayout android:layout_width="fill_parent"
                android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
  <FrameLayout android:id="@id/count"
               android:background="?attr/preferenceCategoryBackground"
               android:layout_width="fill_parent"
               android:layout_height="wrap_content">
    <TextView android:layout_gravity="bottom"
              android:id="@id/startup_count"
              android:paddingBottom="10.0dp"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:textAllCaps="true"
              style="@style/Miuix.AppCompat.TextAppearance.PreferenceCategory" />
  </FrameLayout>
  <Button android:id="@id/btn"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:layout_marginLeft="?android:attr/listPreferredItemPaddingLeft"
          android:layout_marginTop="?android:attr/listPreferredItemPaddingLeft"
          android:layout_marginRight="?android:attr/listPreferredItemPaddingRight"
          android:layout_marginBottom="?android:attr/listPreferredItemPaddingLeft"
          android:text="@string/man_add_application"
          android:layout_alignParentBottom="true"
          style="@style/Widget.Button.Positive" />
  <FrameLayout android:id="@id/prefs_container"
               android:layout_width="fill_parent"
               android:layout_height="fill_parent"
               android:layout_above="@id/btn"
               android:layout_below="@id/count" />
  <TextView android:id="@android:id/empty"
            android:paddingBottom="50.0dp"
            android:visibility="invisible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/power_autorun_apps_empty"
            android:drawableTop="@drawable/icon_list_empty"
            android:drawablePadding="20.0px"
            android:layout_centerInParent="true" />
</RelativeLayout>