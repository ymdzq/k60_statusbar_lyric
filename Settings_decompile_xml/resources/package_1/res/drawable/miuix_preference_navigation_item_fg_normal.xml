<?xml version='1.0' encoding='utf-8' ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:left="@dimen/miuix_preference_navigation_item_mask_padding_horizontal"
        android:top="@dimen/miuix_preference_navigation_item_mask_padding_vertical"
        android:right="@dimen/miuix_preference_navigation_item_mask_padding_horizontal"
        android:bottom="@dimen/miuix_preference_navigation_item_mask_padding_vertical">
    <shape android:shape="rectangle">
      <solid android:color="?attr/selectablePreferenceItemColorNormal" />
      <padding android:left="@dimen/miuix_preference_navigation_item_content_padding_start"
               android:right="@dimen/miuix_preference_navigation_item_content_padding_end" />
      <corners android:radius="@dimen/miuix_preference_navigation_item_mask_radius" />
    </shape>
  </item>
</layer-list>